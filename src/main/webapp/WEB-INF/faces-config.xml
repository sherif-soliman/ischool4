<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
<!-- 
      <factory>
        <exception-handler-factory>
            stream.common.exception.CustomExceptionHandlerFactory
        </exception-handler-factory>
    </factory>
 -->      
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.login.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>security</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.security.SecurityBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>regBean</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.registration.RegistrationBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>exercise</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.question.QuestionBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>exerciseList</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.exercise.ExerciseListBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>subjectExerciseList</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.exercise.SubjectExerciseListBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>examList</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.exam.ExamListBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>homeworkList</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.homework.HomeworkListBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>posts</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.posts.PostsBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>	
	<managed-bean>
		<managed-bean-name>followup</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.followup.FollowUpBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>	
	<managed-bean>
		<managed-bean-name>practiceMgrBean</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.backend.PracticeManagerBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>	
	<managed-bean>
		<managed-bean-name>friendsBean</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.friends.FriendsBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>adminBean</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.admin.AdminBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>	
	<managed-bean>
		<managed-bean-name>babBean</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.setup.BabBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>	
	<managed-bean>
		<managed-bean-name>performanceBean</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.setup.PerformanceBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>	

	<managed-bean>
		<managed-bean-name>examBuilderBean</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.setup.ExamBuilderBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>	

	<managed-bean>
		<managed-bean-name>lookupSetupBean</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.setup.LookupSetupBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>	
	<managed-bean>
		<managed-bean-name>homeBean</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.home.HomeBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>	
	
	<managed-bean>
		<managed-bean-name>reportsListBean</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.reports.ReportsListBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>	

	<managed-bean>
		<managed-bean-name>practiceReportsListBean</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.reports.PracticeReportsListBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>reportDetailsBean</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.reports.ReportDetailsBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>	
	
	<managed-bean>
		<managed-bean-name>activityReportBean</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.monitor.ActivityReportBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>	
	
	<managed-bean>
		<managed-bean-name>perfBean</managed-bean-name>
		<managed-bean-class>stream.ischool.managedBeans.performance.PerformanceBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>	
	
	<navigation-rule>
		<display-name>home</display-name>
		<from-view-id>/w_login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>self</from-outcome>
			<to-view-id>/w_login.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>valid_login</from-outcome>
			<to-view-id>/jsp/home/w_home.jsp</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>m_home</display-name>
		<from-view-id>/m_login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>self</from-outcome>
			<to-view-id>/m_login.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>valid_login</from-outcome>
			<to-view-id>/jsp/home/m_home.jsp</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>question</display-name>
		<from-view-id>/jsp/question/question.jsp</from-view-id>
		<navigation-case>
			<from-outcome>self</from-outcome>
			<to-view-id>/jsp/question/question.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>back_to_exercise</from-outcome>
			<to-view-id>/jsp/exercise/exercise_list.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>back_to_exam</from-outcome>
			<to-view-id>/jsp/exam/exam_list.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>back_to_homework</from-outcome>
			<to-view-id>/jsp/homework/homework_list.jsp</to-view-id>			
		</navigation-case>				
	</navigation-rule>
	<navigation-rule>
		<display-name>homework_questions</display-name>
		<from-view-id>/jsp/homework/homework_questions.jsp</from-view-id>
		<navigation-case>
			<from-outcome>self</from-outcome>
			<to-view-id>/jsp/homework/homework_questions.jsp</to-view-id>			
		</navigation-case>
		<navigation-case>
			<from-outcome>back_to_homework_list</from-outcome>
			<to-view-id>/jsp/homework/homework.jsp</to-view-id>			
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>posts_page</display-name>
		<from-view-id>/jsp/posts/posts_page.jsp</from-view-id>
		<navigation-case>
			<from-outcome>self</from-outcome>
			<to-view-id>/jsp/posts/posts_page.jsp</to-view-id>			
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>registration</display-name>
		<from-view-id>/jsp/registration/registration.jsp</from-view-id>
		<navigation-case>
			<from-outcome>self</from-outcome>
			<to-view-id>/jsp/registration/registration.jsp</to-view-id>			
		</navigation-case>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/w_login.jsp</to-view-id>
			<redirect></redirect>			
		</navigation-case>
	</navigation-rule>		
		
	<navigation-rule>
		<display-name>practice_manager</display-name>
		<from-view-id>/jsp/backend/practice_manager.jsp</from-view-id>
		<navigation-case>
			<from-outcome>open_practice_search</from-outcome>
			<to-view-id>/jsp/backend/practice_search.jsp</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>open_question_search</from-outcome>
			<to-view-id>/jsp/backend/question_search.jsp</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>open_public_images</from-outcome>
			<to-view-id>/jsp/backend/public_images.jsp</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>return to home</from-outcome>
			<to-view-id>/jsp/posts/posts_page.jsp</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>public_images</display-name>
		<from-view-id>/jsp/backend/public_images.jsp</from-view-id>
		<navigation-case>
			<from-outcome>close_public_images</from-outcome>
			<to-view-id>/jsp/backend/practice_manager.jsp</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>practice_search</display-name>
		<from-view-id>/jsp/backend/practice_search.jsp</from-view-id>
		<navigation-case>
			<from-outcome>close</from-outcome>
			<to-view-id>/jsp/backend/practice_manager.jsp</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>question_search</display-name>
		<from-view-id>/jsp/backend/question_search.jsp</from-view-id>
		<navigation-case>
			<from-outcome>close</from-outcome>
			<to-view-id>/jsp/backend/practice_manager.jsp</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>w_logout</display-name>
		<from-view-id>/jsp/home/w_home.jsp</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/w_login.jsp</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<display-name>m_logout</display-name>
		<from-view-id>/jsp/home/m_home.jsp</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/m_login.jsp</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>view_full_list</display-name>
		<from-view-id>/jsp/reports/reports_list.jsp</from-view-id>
		<navigation-case>
			<from-outcome>view_class_homework_report_list</from-outcome>
			<to-view-id>/jsp/reports/practice_reports_list.jsp?practice_type=Class_Homework</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>view_full_exercise_report_list</from-outcome>
			<to-view-id>/jsp/reports/practice_reports_list.jsp?practice_type=Exercise</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>view_full_homework_report_list</from-outcome>
			<to-view-id>/jsp/reports/practice_reports_list.jsp?practice_type=Homework</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>view_full_exam_report_list</from-outcome>
			<to-view-id>/jsp/reports/practice_reports_list.jsp?practice_type=Exam</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	
</faces-config>
